{
  "name": "nx-oxlint-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "projectType": "application",
    "sourceRoot": "tools/nx-oxlint-e2e/src",
    "implicitDependencies": [
      "nx-oxlint"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/vite:test",
        "outputs": [
          "{projectRoot}/test-output/vitest/coverage"
        ],
        "options": {
          "config": "tools/nx-oxlint-e2e/vite.config.ts",
          "run": true
        },
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "devDependencies": {
    "nx-oxlint": "^0.0.0-e2e"
  }
}
